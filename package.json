{
  "name": "gitline",
  "version": "0.0.2",
  "description": "Graphical git UI",
  "author": "Piotr Placzek <piotrpdev@gmail.com>",
  "private": true,
  "keywords": [
    "git",
    "ui",
    "graph",
    "network"
  ],
  "scripts": {
    "dev": "vite",
    "dev:demo": "vite build && vite --config vite-demo.config.ts",
    "build": "tsc && vite build",
    "build:demo": "vite build && vite build --config vite-demo.config.ts",
    "test": "vitest --dir src/test"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.1.3",
    "@types/node": "^20.8.10",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vite-plugin-static-copy": "^0.17.0",
    "vitest": "^0.34.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/piotrpdev/gitline.git"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "./dist/gitline.umd.cjs",
  "module": "./dist/gitline.js",
  "exports": {
    ".": {
      "import": "./dist/gitline.js",
      "require": "./dist/gitline.umd.cjs"
    }
  },
  "dependencies": {
    "crypto-js": "^4.2.0"
  }
}
